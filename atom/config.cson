"*":
  Zen: {}
  "autocomplete-plus":
    fileBlacklist: [
      "*.md"
      "*.txt"
    ]
  core:
    disabledPackages: [
      "metrics"
      "exception-reporting"
      "vim-mode"
      "emmet"
    ]
  editor:
    fontSize: 12
    invisibles: {}
    lineHeight: 1.3
  "exception-reporting":
    userId: "82cd7f15-1e6d-e5bc-38b7-7e65f7159532"
  "find-and-replace":
    openProjectFindResultsInRightPane: true
    scrollToResultOnLiveSearch: true
  linter:
    subtleLinterErrors: [
      "jshint"
      "scss-lint"
    ]
  "linter-jshint": {}
  metrics:
    userId: "5977601d5d0e2c0fb47882fc311f352130f8bba8"
  "toggle-packages":
    togglePackages: [
      "vim-mode"
      "emmet"
    ]
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
  "vim-mode":
    useClipboardAsDefaultRegister: true
    useSmartcaseForSearch: true
  welcome:
    showOnStartup: false
